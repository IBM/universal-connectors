{
  "plugin_name": "mssql over JDBC",
  "help_link": "https://github.com/IBM/universal-connectors/blob/main/filter-plugin/logstash-filter-mssql-guardium/README.md",
  "input_name": "JDBC_input",
  "input_parameters": [
    {
      "jdbc_connection_string": {
        "type": "text",
        "label": "Connection string"
      },
      "last_run_metadata_path": {
        "type": "text",
        "label": "Last run metadata path"
      },
      "jdbc_driver_class": {
        "type": "text",
        "default": "com.microsoft.sqlserver.jdbc.SQLServerDriver",
        "hidden": true
      },
      "schedule": {
        "type": "text",
        "default": "*/2 * * * *",
        "hidden": true
      },
      "use_column_value": {
        "type": "boolean",
        "default": true,
        "hidden": true
      },
      "tracking_column": {
        "type": "text",
        "default": "updatedeventtime",
        "hidden": true
      },
      "tracking_column_type": {
        "type": "text",
        "default": "numeric",
        "hidden": true
      },
      "statement_select": {
        "type": "text",
        "default": "server_instance_name,event_time, session_id, database_name, client_ip, server_principal_name, application_name, statement, succeeded, DATEDIFF_BIG(ns, '1970-01-01 00:00:00.00000', event_time) AS updatedeventtime",
        "label": "SELECT"
      },
      "statement_from": {
        "type": "text",
        "default": "sys.fn_get_audit_file('C:\\AuditLogFiles\\*.sqlaudit', DEFAULT, DEFAULT)",
        "label": "FROM"
      },
      "statement_where": {
        "type": "text",
        "default": "schema_name not in ('sys') and object_name NOT IN ('dbo','syssubsystems','fn_sysdac_is_currentuser_sa','backupmediafamily','backupset','syspolicy_configuration','syspolicy_configuration_internal','syspolicy_system_health_state','syspolicy_system_health_state_internal','fn_syspolicy_is_automation_enabled','spt_values','sysdac_instances_internal','sysdac_instances') and database_principal_name not in('public') and ((succeeded =1) or (succeeded =0 and statement like '%Login failed%' )) and statement != '' and DATEDIFF_BIG(ns, '1970-01-01 00:00:00.00000', event_time) > :sql_last_value order by event_time",
        "label": "WHERE"
      },
      "plugin_timezone": {
        "type": "text",
        "default": "utc",
        "hidden": true
      },
      "enrollmentId": {
        "type": "text",
        "default": "NA",
        "hidden": true
      },
      "server_name": {
        "type": "text",
        "label": "Server name"
      },
      "account_id": {
        "type": "text",
        "default": "NA",
        "hidden": true
      },
      "server_host_name": {
        "type": "text",
        "default": "NA",
        "hidden": true
      },
      "jdbc_driver_library": {
        "type": "text",
        "default": "${LOGSTASH_DIR}/logstash-core/lib/jars/MSSQL-jdbc.jar",
        "hidden": true
      },
      "jdbc_validate_connection": {
        "type": "boolean",
        "default": false,
        "hidden": true
      },
      "type": {
        "type": "text",
        "default": "MSSQL",
        "hidden": true
      }
    }
  ],
  "credentials": [
    {
      "jdbc_password": {
        "type": "password",
        "label": "password"

      }
    }
  ],
  "filter_name": "Guardium_Mssql_filter",
  "filter_parameters": [],
  "auth_parameters": [
    {
      "guc_input_param_jdbc_user": {
        "type": "text",
        "label": "jdbc_user"
      }
    }
  ],
  "supported_platforms": ["on-premise"]
}