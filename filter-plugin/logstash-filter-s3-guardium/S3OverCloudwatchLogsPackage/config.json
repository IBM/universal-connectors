{
  "connection": {
    "type": {
      "displayName": "S3 Over Cloudwatch Logs",
      "type": "connectorType"
    },
    "connectionMethod": {
      "type": "text",
      "default": "GUC",
      "hidden": true
    },
    "help": {
      "default": "https://github.com/IBM/universal-connectors/blob/main/filter-plugin/logstash-filter-s3-guardium/README.md",
      "type": "link"
    }
  },
  "configuration": {
    "guc_input_name": {
      "type": "text",
      "default": "Cloudwatch_logs_input",
      "hidden": true
    },
    "guc_input_param_role-arn": {
      "type": "text",
      "optional": true
    },
    "guc_input_param_start_position": {
      "type": "text",
      "default": "end",
      "hidden": true
    },
    "guc_input_param_interval": {
      "type": "number",
      "default": 5,
      "hidden": true
    },
    "guc_input_param_access_key_id": {
      "type": "password"
    },
    "guc_input_param_region": {
      "type": "text"
    },
    "guc_input_param_secret_access_key": {
      "type": "password"
    },
    "guc_input_param_event_filter": {
      "type": "text",
      "optional": true
    },
    "guc_input_param_type": {
      "type": "text",
      "default": "S3",
      "hidden": true
    },
    "guc_filter_name": {
      "type": "text",
      "default": "Guardium_S3_filter",
      "hidden": true
    },
    "auth": {
      "type": "fields",
      "guc_input_param_log_group": {
        "type": "text"
      }
    }
  }
}

