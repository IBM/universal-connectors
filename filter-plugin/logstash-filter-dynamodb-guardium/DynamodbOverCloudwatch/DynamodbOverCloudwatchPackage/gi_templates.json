{
  "plugin_name": "DynamoDB Over Cloudwatch_Logs",
  "help_link": "https://github.com/IBM/universal-connectors/blob/main/filter-plugin/logstash-filter-dynamodb-guardium/README.md",
  "input_name": "Cloudwatch_logs_input",
  "input_parameters": [
    {
      "role-arn": {
        "type": "text",
        "optional": true,
        "label": "AWS Role ARN (optional)"
      },
      "start_position": {
        "type": "text",
        "default": "end",
        "hidden": true
      },
      "interval": {
        "type": "number",
        "default": 5,
        "label": "Interval"
      },
      "region": {
        "type": "text",
        "label": "AWS account region"
      },
      "event_filter": {
        "type": "text",
        "optional": true,
        "label": "Event filter (optional)"
      },
      "account_id": {
        "type": "text",
        "label": "Account id"
      },
      "type": {
        "type": "text",
        "default": "dynamodb",
        "hidden": true
      }
    }
  ],
  "credentials": [
    {
      "access_key_id": {
        "type": "password",
        "optional": true,
        "label": "AWS access key id"
      },
      "secret_access_key": {
        "type": "password",
        "optional": true,
        "label": "AWS secret access key"
      }
    }
  ],
  "filter_name": "Guardium_DynamoDB_filter",
  "filter_parameters": [],
  "auth_parameters": [
    {
      "guc_input_param_log_group": {
        "type": "text",
        "label": "Cloudwatch Log Group name"
      }
    }
  ],
  "supported_platforms": ["AWS"]
}