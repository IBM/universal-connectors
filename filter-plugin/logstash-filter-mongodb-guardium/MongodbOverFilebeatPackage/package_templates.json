{
  "plugin_name": "MongoDB Over Filebeat",
  "help_link": "https://github.com/IBM/universal-connectors/blob/main/filter-plugin/logstash-filter-mongodb-guardium/README.md",
  "input_name": "Filebeat_input",
  "input_parameters": [
    {
      "port": {
        "type": "number",
        "gi": {
          "default": 5044,
          "hidden": true
        },
        "gdp": {
          "default": 5044,
          "hidden": false
        }
      },
      "is_ssl": {
        "type": "boolean",
        "gi": {
          "default": true,
          "hidden": true
        },
        "gdp": {
          "default": false,
          "hidden": false
        }
      },
      "include_codec_tag": {
        "type": "boolean",
        "gi": {
          "default": true,
          "hidden": true
        },
        "gdp": {
          "default": true,
          "hidden": true
        }
      },
      "ssl_cert_auth": {
        "type": "text",
        "gi": {
          "default": "SSL_CERT_AUTH",
          "hidden": true
        },
        "gdp": {
          "hidden": true,
          "optional": true
        }
      },
      "ssl_crt_path": {
        "type": "text",
        "gi": {
          "default": "/service/certs/external/tls.crt",
          "hidden": true
        },
        "gdp": {
          "hidden": true,
          "optional": true
        }
      },
      "ssl_key_path": {
        "type": "text",
        "gi": {
          "default": "/service/certs/external/tls.key",
          "hidden": true
        },
        "gdp": {
          "hidden": true,
          "optional": true
        }
      },
      "pair_id": {
        "type": "text",
        "default": "mongoOverFilebeat",
        "hidden": true
      }
    }
  ],
  "filter_name": "Guardium_MongoDB_filter",
  "filter_parameters": [
    {
      "pair_id": {
        "type": "text",
        "default": "mongoOverFilebeat",
        "hidden": true
      }
    }
  ],
  "auth_parameters": [
    {
      "guc_filter_param_datasource_tag": {
        "param_type": "filter",
        "type": "text",
        "label": "Datasource Tag",
        "description": "Tag that was added to every event sent from the data source."
      }
    }
  ],
  "supported_platforms": ["on-premise"]
}