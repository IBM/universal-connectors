{
  "plugin_name": "OUA over JDBC connect",
  "help_link": "https://github.com/IBM/universal-connectors/blob/main/filter-plugin/logstash-filter-oua-guardium/README.md",
  "kafka_connect_name": "Kafka connect JDBC",
  "kafka_connect_parameters": [
    {
      "connector_class": {
        "type": "text",
        "default": "io.aiven.connect.jdbc.JdbcSourceConnector",
        "hidden": true,
        "optional": false,
        "label": "Connector class",
        "sequence": 11
      },
      "tracking_column_type": {
        "type": "text",
        "default": "timestamp",
        "hidden": true,
        "label": "Tracking column type",
        "optional": false,
        "sequence": 9
      },
      "tracking_column": {
        "type": "text",
        "default": "EVENT_TIMESTAMP_UTC",
        "hidden": true,
        "label": "Tracking column name",
        "optional": false,
        "sequence": 10
      },
      "timestamp_initial_ms": {
        "type": "number",
        "default": 0,
        "hidden": false,
        "label": "Initial time (milliseconds)",
        "optional": false,
        "sequence": 4
      },
      "service_name": {
        "type": "text",
        "default": "",
        "hidden": false,
        "label": "Service name / SID",
        "optional": false,
        "sequence": 7
      },
      "query": {
        "type": "text",
        "default": "SELECT EVENT_TIMESTAMP_UTC, To_char(sessionid) AS SESSION_ID, dbusername AS USER_ID, ( CASE WHEN EXECUTION_ID IS NOT NULL THEN NULL ELSE ( SELECT GLOBAL_NAME FROM CONTAINERS (GLOBAL_NAME) WHERE CON_ID IN ( SELECT CON_ID FROM V$MYSTAT ) ) END ) CON_NAME, os_username AS OS_USER, Substr(Regexp_substr(authentication_type, 'HOST=([^)]*)'), 6) AS CLIENT_HOST_IP, userhost AS CLIENT_HOST_NAME, (SELECT Sys_context('USERENV', 'SERVER_HOST_IP') FROM dual) SERVER_HOST_IP, client_program_name, To_char(dbid) AS DB_ID, (SELECT sys_context('USERENV', 'DB_NAME') FROM dual) DB_NAME, To_char(return_code) AS RETURN_CODE, SQL_TEXT FROM unified_audit_trail",
        "hidden": false,
        "label": "Query",
        "optional": false,
        "sequence": 8
      },
      "datatype": {
        "type": "text",
        "default": "OUA",
        "hidden": true,
        "label": "Datatype",
        "optional": false,
        "sequence": 12
      },
      "value_converter": {
        "type": "text",
        "default": "org.apache.kafka.connect.converters.ByteArrayConverter",
        "hidden": true,
        "label": "Record Value Converter",
        "optional": false,
        "sequence": 13
      },
      "key_converter": {
        "type": "text",
        "default": "org.apache.kafka.connect.storage.StringConverter",
        "hidden": true,
        "label": "Record Key Converter",
        "optional": false,
        "sequence": 14
      }
    }
  ],
  "input_name": "Kafka input",
  "input_parameters": [
    {
      "topic": {
        "type": "text",
        "hidden": true,
        "default": "DEFAULT_TOPIC_NAME",
        "optional": false,
        "label": "Topic",
        "sequence": 4
      },
      "bootstrap_servers": {
        "type": "text",
        "default": "DEFAULT_BROKERS",
        "label": "Brokers of kafka cluster",
        "optional": false,
        "hidden": true,
        "sequence": 2
      }
    }
  ],
  "filter_name": "Guardium_OUA_filter",
  "filter_parameters": [
    {
      "host": {
        "type": "text",
